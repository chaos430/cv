string[n]\r\n        }\r\n        html.innerHTML = string2;\r\n        style.innerHTML = string.substring(0, n);\r\n        window.scrollTo(0, 99999);\r\n        html.scrollTo(0, 99999);\r\n        if (n < string.length - 1) {\r\n            //n不是最后一个，就继续\r\n            n += 1;\r\n            step();\r\n        }\r\n    }, 50);\r\n};\r\n\r\nstep();\r\n\r\nsetTimeout(() => {\r\n    style.innerHTML = `\r\nbody{\r\n    color: red;\r\n}\r\n`;\r\n}, 3000);"],"names":["$16b5ad875ae907e2f7f79e7b8fe116cc$var$html","document","querySelector","$16b5ad875ae907e2f7f79e7b8fe116cc$var$style","$16b5ad875ae907e2f7f79e7b8fe116cc$var$string","$16b5ad875ae907e2f7f79e7b8fe116cc$var$string2","$16b5ad875ae907e2f7f79e7b8fe116cc$var$n","$16b5ad875ae907e2f7f79e7b8fe116cc$var$step","setTimeout","innerHTML","substring","window","scrollTo","length"],"version":3,"file":"index.HASH_REF_83d4711ea689fc47f03deb70f03c99b4.js.map"}